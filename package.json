{
  "name": "sparks-backend",
  "version": "1.0.44",
  "description": "sparks backend",
  "engines": {
    "node": "6.1.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "heroku-postbuild": "npm run build",
    "build": "tsc && babel -d dist dist",
    "test": "bin/test",
    "testci": "tsc && parallel --no-notice -j4 \"node --no-warnings {} 2>/dev/null | tap-xunit 1> ${$CIRCLE_TEST_REPORTS}/{#}.xml\" ::: dist/**/*.js",
    "watch": "fswatch -0 -Ei \".*\\.ts$\" -Ee \".*\\.ts\" src | xargs -0 -n1 -I {} bash -c \"clear && ts-node {} | tap-min\"",
    "lint": "node_modules/.bin/tslint src/**/*.ts",
    "commit": "git-cz",
    "version": "npm run changelog",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SparksNetwork/sparks-backend.git"
  },
  "author": "'sdebaun' <'sdebaun74@gmail.com'>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sdebaun/sparks-backend/issues"
  },
  "homepage": "https://github.com/sdebaun/sparks-backend#readme",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    },
    "ghooks": {
      "commit-msg": "node ./node_modules/validate-commit-msg/index.js"
    }
  },
  "greenkeeper": {
    "ignore": [
      "sendgrid",
      "firebase",
      "seneca"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "blue-tape": "^1.0.0",
    "commitizen": "^2.8.6",
    "conventional-changelog": "^1.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "cz-customizable": "^4.0.0",
    "ghooks": "^1.3.2",
    "nodemon": "^1.10.1",
    "source-map-support": "^0.4.3",
    "tap": "^7.1.2",
    "tap-xunit": "^1.4.0",
    "tape": "^4.6.1",
    "tape-async": "^2.1.0",
    "tslint": "^3.15.1",
    "validate-commit-msg": "^2.8.2"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "braintree": "^1.41.0",
    "braintree-node": "^1.1.1",
    "express": "^4.13.4",
    "firebase": "^2.4.1",
    "firebase-queue": "^1.2.1",
    "inflection": "^1.10.0",
    "moment": "^2.15.1",
    "moment-timezone": "^0.5.4",
    "node-uuid": "^1.4.7",
    "prfun": "^2.1.3",
    "ramda": "^0.21.0",
    "rx": "^4.0.8",
    "sendgrid": "^4.2.1",
    "seneca": "^2.1.0",
    "seneca-await": "0.0.12",
    "sinon": "^1.17.6",
    "ts-node": "^1.5.2",
    "typescript": "^2.1.0-dev.20160804",
    "typings": "^1.4.0"
  }
}
